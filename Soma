public class Soma {
    public int A;
    public int B;
    public int soma;

    public int getA(){
        return A;
    }

    public void setA(int A){
        this.A = A;
    }

    public int getB(){
        return B;
    }

    public void setB(int B){
        this.B = B;
    }

    public int getSoma(){
        return soma;
    }

    public void Soma(){
        soma = A+B;

        System.out.printf("O resultado de %d + %d Ã© igual a %d",A,B,soma);
    }

}
